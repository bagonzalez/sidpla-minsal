{% for proUniA in proUnisalActual %}
  {% for  area in  areas%}
<div id="{{"now" | date("Y")}}{{area.getCodArea}}Div">
    <table border="1" WIDTH='85%'>
        <thead>
            <tr  style=" font-size: 14px;">
                <th><strong>  Cod </strong>
                </th>
                <th><strong>  Objetivo </strong>
                </th>
                <th colspan="3"><strong>  Acciones </strong>
                </th>
            </tr>
        </thead>
        <tbody>
    {%for obj in objTemplateDao.obtenerPorArea(area.getCodArea ,proUniA.getObjeEspeProgra)%}
                <tr>
                    <td style=" font-size: 12px;">{{ obj.nomenObjEspUni}}
                    </td>
                    <td style=" font-size: 12px;" >{{ obj.getDescObjEspUni}}
                    </td>
                    <td class="tdBotonesIconos" WIDTH='60' >
                        <input width="50" height="50" type="image" src="{{ asset('/images/iconos/editar.png')}}"  id="editar" value="Editar" onclick="$('#idObj').attr('value',{{obj.getcodObjEspUni}});$('#anio').attr('value', $('#selanio').val());$('#idArea').attr('value', $('#selarea').val());this.form.action='{{path('MinSalSidPlaTemplateUnisalBundle_editarObjetivoEspeUnisal')}}';" />
                        <br/>Editar
                    </td>
                    <td class="tdBotonesIconos" WIDTH='60' >
                        <input width="50" height="50" type="image" src="{{ asset('/images/iconos/gestion.png')}}" onclick="$('#idObj').attr('value',{{obj.getcodObjEspUni}});this.form.action='{{path('MinSalSidPlaTemplateUnisalBundle_mostrarResEspeUnisal')}}';" />
                        <br/>Resultados Esperados
                    </td>
                    <td class="tdBotonesIconos" WIDTH='60' >
                        <input width="50" height="50" type="image" src="{{ asset('/images/iconos/gestion.png')}}" onclick="$('#idObj').attr('value',{{obj.getcodObjEspUni}});this.form.action='{{path('MinSalSidPlaIndicadoresTemplateBundle_mostrarIndicadoresSalud')}}';" />
                        <br/>Indicadores de Salud

                    </td>
                </tr>
    {% endfor%}
            </tbody>
        </table>
    </div> 
  {% endfor%}
{% endfor%}


